{
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "squizlabs/php_codesniffer",
                "version": "master",
                "source": {
                    "url": "https://github.com/squizlabs/PHP_CodeSniffer.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
